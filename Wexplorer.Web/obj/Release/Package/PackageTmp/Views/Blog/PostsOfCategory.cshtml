@model Wexplorer.Web.Models.PostsModel
@{
    ViewBag.Canonical = "http://wexplorer.ru/" + Model.Posts.Select(s => s.Category.UrlCategory).First();
    ViewBag.Title = Model.Posts.Select(s => s.Category.NameCategory).First();
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div itemscope itemtype="http://schema.org/Blog" class="posts">
    @Html.Partial("BlogPosts", Model.Posts)
    <div class="pages">
        @Html.PageLinks((Wexplorer.Web.Models.PagingInfo)Model.PagingInfo, x => Url.Action("PostsOfCategory", new { cat = Model.Posts.Select(s => s.Category.UrlCategory).First(), page = x }))
    </div>
</div>

